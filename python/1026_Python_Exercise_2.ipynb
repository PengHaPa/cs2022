      "source": [
        "def fib(n):\n",
        "  fib1 = 0\n",
        "  fib2 = 1\n",
        "  for n in range(0,n):\n",
        "    result = fib1 + fib2\n",
        "    fib1 = fib2\n",
        "    fib2 = result\n",
        "\n",
        "  return result\n",
        "  \n",
        "fib(7)"
      ],
